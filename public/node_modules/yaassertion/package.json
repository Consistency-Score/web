    "babel-eslint": "^8.2.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "core-js": "^2.5.3",
    "eslint": "^4.17.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "mocha": "^5.0.0",
    "prettier": "^1.10.2",
    "rollup": "^0.56.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-replace": "^2.0.0"
