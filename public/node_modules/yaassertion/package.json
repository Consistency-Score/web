{
  "name": "yaassertion",
  "version": "1.0.2",
  "description": "Yet another JavaScript assertion/invariant library.",
  "main": "dist/index.js",
  "module": "src/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "BABEL_ENV=client rollup --config src/rollup-config.js",
